_muxer_jpeg_ = {
  muxer_id                  = 8,  -- do not modify
  stream_id                 = 3,
  buffer_pkt_num            = 3,
  buffer_size               = 3, --Unit MB. For buffering jpeg data.
  file_name_prefix          = "Amba",
  muxer_attr                = "event",
  max_file_number           = 32,   --This is the max file number in a directory
