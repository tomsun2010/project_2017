_audio_source_ = {
  name             = "AudioSource-16K",
  rt_config        = {
    enabled        = false,
    priority       = 10,
  },
  packet_pool_size = 64, -- Audio pool size
  codec_enable     = true,
  initial_volume   = 90, -- Initial volume (0 ~ 100, -1 means default)
  audio_profile    = "high",
  audio_type       = {
                       highest = {
                           "aac-48k",
                           "opus-48k",
                           --"speex-48k",
                       },

                       high = {
                            "aac-16k",
                            "opus-16k",
                            "g711-16k",
                            --"speex-16k",
                       },

                       low  = {
                            "g711-8k",
                            "g726-8k",
                            --"aac-8k",
                            --"opus-8k",
                            --"speex-8k",
                       },
                     },
  interface        = "pulse",
  enable_aec       = false, -- Enable software echo cancel, for debug only
}

return _audio_source_
