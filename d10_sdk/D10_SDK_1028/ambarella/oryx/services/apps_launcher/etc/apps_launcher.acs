apps = {
  {
    filename ="img_svc",
    type = 4,
    enable  = true
  },
  {
    filename ="media_svc",
    type = 5,
    enable  = true
  },
  {
    filename ="audio_svc",
    type = 2,
    enable  = true
  },
  {
    filename ="video_svc",
    type = 8,
    enable  = true,
 -- video_svc handles motion event to do SmartAVC (bitrate adjustment)
    event_handle_motion = true
  },
  {
    filename ="event_svc",
    type = 3,
    enable  = true
  },
  {
    filename ="net_svc",
    type = 6,
    enable  = true
  },
  {
    filename ="sys_svc",
    type = 7,
    enable  = true,
  },
  {
  -- usr_svc is an example and place holder, but by default disabled
    filename ="usr_svc",
    type = 14,
    enable = false,
  },
  {
    filename ="rtsp_svc",
    type = 9,
    enable = true,
  },
  {
    filename ="sip_svc",
    type = 10,
    enable = false,
  },
  {
    filename ="playback_svc",
    type = 11,
    enable = true,
  },
  {
    filename ="video_edit_svc",
    type = 12,
    enable = false,
  },
  {
    filename ="efm_src_svc",
    type = 13,
    enable = true,
  },
}

return apps
